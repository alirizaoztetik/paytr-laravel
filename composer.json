{
    "name": "turown/laravel-paytr",
    "description": "PayTR integration for Laravel.",
    "keywords": ["laravel", "paytr"],
    "license": "MIT",
    "authors": [
        {
            "name": "Ali Rıza Öztetik",
            "email": "info@alirizaoztetik.com"
        }
    ],
    "require": {
       "php": "^8.1|^8.2|^8.3",
       "illuminate/console": "^10.0|^11.0",
       "illuminate/support": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "PayTR\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "PayTR\\PaytrServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "orchestra/testbench": "^8.0",
        "phpstan/phpstan": "^1.10"
    }
}
